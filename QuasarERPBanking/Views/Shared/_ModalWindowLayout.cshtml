@using AnaliticoPresupuesto.Models
<!DOCTYPE html>
<html>
<head>
    <title>Quasar - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/jquery-ui-1.9.2.custom.css")" rel="stylesheet" type="text/css" />
    
    <script src="@Url.Content("~/Scripts/jquery-1.8.3.js")" type="text/javascript"></script>
    <script type='text/javascript' src='@Url.Content("~/Scripts/jquery-ui-1.9.2.custom.js")'></script>
    <script type='text/javascript' src="@Url.Content("~/Scripts/basic.js?version=1.0.2")"></script>
</head>
<body style="background-color:#FFF;">
    <div class="page">
             @{
                AlertMessage am = (AlertMessage)ViewBag.AlertMessage;
                if (am != null && am.HayMensaje())
                {
                    string msg = am.getMessage();
                    <span class="field-validation-error" style="color:#000; font-size:x-large;">@msg</span>
                    <br />
                }
            }

            @RenderBody()
    </div>
    @{
        Html.RenderPartial("Timeout");       
    }
</body>
</html>
